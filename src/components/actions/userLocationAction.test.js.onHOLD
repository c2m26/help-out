import * as actions from './userLocationAction'
import { GET_USER_LOCATION } from './types'
import userLocatioAction, { getUserLocation } from './userLocationAction'


it('should create an action that gets the user location', () => {
  const expectedAction = {
    type: GET_USER_LOCATION
    }

  const onAction = expect.spyOn(actions, 'getUserLocation').andCallThrough();

  expect(actions.getUserLocation).toReturnWith(expectedAction)

  onAction.mockRestore();
})